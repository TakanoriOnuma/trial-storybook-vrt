{
  "compilerOptions": {
    "incremental": true,
    "declaration": true,
    "target": "es2015",
    "module": "commonjs",
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "node",
    "sourceMap": true,
    "stripInternal": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true
  }
}
